{
  "name": "Wazuh-file-encryptor",
  "version": "1.0",
  "author": "SP",
  "url": "https://github.com",
  "license": "V3",
  "description": "Encrypt a file on a host via Wazuh agent",
  "dataTypeList": ["thehive:case", "thehive:case_artifact"],
  "command": "Wazuh-file-encryptor/wazuh-en.py",
  "baseConfig": "Wazuh-file-encryptor",
  "configurationItems": [
    {
      "name": "wazuh_manager",
      "description": "URL for Wazuh Manager",
      "type": "string",
      "multi": false,
      "required": true,
      "defaultValue": "https://localhhost:55000"
    },
    {
      "name": "wazuh_user",
      "description": "User for Wazuh Manager",
      "type": "string",
      "multi": false,
      "required": true,
      "defaultValue": "foo"
    },
    {
      "name": "wazuh_password",
      "description": "Password for Wazuh Manager",
      "type": "string",
      "multi": false,
      "required": true,
      "defaultValue": "bar"
    }
  ]
}
